<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".activity.JetpackActivity">
    <data>
        <import type="com.example.kotlinstudy.viewmodel.ShareViewModel" />
        <import type="java.util.List"/>
        <import type="java.util.Map"/>
        <variable
            name="user"
            type="ShareViewModel" />
        <variable
            name="index"
            type="int" />
        <variable
            name="list"
            type="List&lt;String&gt;" />
        <variable
            name="map"
            type="Map&lt;String,String&gt;" />
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <EditText
            android:id="@+id/jetpack_et"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Jetpack"
            />

        <TextView
            android:id="@+id/jetpack_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Jetpack"
            />
        <TextView
            android:id="@+id/jetpack_tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@{user.getShareData()}"
            />
        <Button
            android:id="@+id/jetpack_btn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="refresh"
            />

        <Button
            android:id="@+id/jetpack_btn2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="insert"
            />

        <Button
            android:id="@+id/jetpack_btn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="delete"
            />

        <Button
            android:id="@+id/jetpack_btn4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="map/list/set"
            />
        <Button
            android:id="@+id/jetpack_btn5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ThreadPool"
            />
        <Button
            android:id="@+id/jetpack_btn6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cache"
            />
        <Button
            android:id="@+id/jetpack_btn7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="viewmoel"
            />
        <Button
            android:id="@+id/jetpack_btn8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="scope"
            />
        <Button
            android:id="@+id/jetpack_btn9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="jackutils lib"
            />
        <Button
            android:id="@+id/jetpack_btn10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ARouter"
            />
        <Button
            android:id="@+id/jetpack_btn11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="reflect+proxy"
            />
    </LinearLayout>
</layout>